{
  "name": "vue-ssr-setup",
  "version": "1.0.0",
  "description": "Simple Vue SSR Setup",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "nodemon": "nodemon nodemon.js",
    "dev-client": "webpack serve --config build/webpack.client.js",
    "dev-server": "webpack -w --config build/webpack.server.js",
    "dev": "run-p dev-client dev-server nodemon",
    "clean": "rimraf ./dist/",
    "build-client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js",
    "build-server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js",
    "build-all": "run-p build-client build-server",
    "build": "run-s clean build-all",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "vue": "^2.6.12",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs": "^0.0.1-security",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "sass-loader": "^10.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "4.46.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
}
